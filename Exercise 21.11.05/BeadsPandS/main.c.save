#include <stdio.h>
#include <stdlib.h>

char* getPattern (const char* str);

int main()
{
    printf("Hello world!\n");
    return 0;
}

char* getPattern (const char* str)
{
    int patternLen = 0;
    for (int i = 0; str[i] != '\0'; i ++)
    {
        if (str[i] != str[i % patternLen])
            patternLen = i;
    }
}
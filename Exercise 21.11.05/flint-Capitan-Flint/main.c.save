#include <stdio.h>
#include <stdlib.h>

const char* direction[5] = {"North",
                            "South",
                            "East" ,
                            "West" ,
                            "Treasure!"};

int cmp(const void *keyval, const void *datum);

int main()
{
    char str[100] = {};
    int arg = 0;
    int x =0, y = 0;

    char** dir = 0;
    do
    {
        scanf ("%s %d", str, arg);
        dir = bsearch (str, direction, 5, sizeof (char*), cmp);
        
        switch (dir)
        {
            case 0 :
                y += arg;
                break;
            case 1 :
                y -= arg;
                break;
            case 2 :
                x += arg;
                break;
            case 3 :
                x -= arg;
                break;
        }
    }
    while (dir != direction[4]);
    printf("%d %d\n", x, y);
    return 0;
}

int cmp(const void *key, const void *data)
{
    return -strcmp (*((const char**)key), *((const char**)data));
}

#include <stdio.h>
#include <stdlib.h>

#define N 1000

char * stringtolower(char * s);

int main()
{
    char s[N + 1] = {};
    char isBomb = 0;

    while (scanf ("%1000s", s) == 1)
    {
        stringToLower (s);
        if (strstr (s, "bomb") != NULL)
            isBomb = 1;
    }

    printf ("%s", (isBomb)? "YES" : "NO");
    return 0;
}

char * stringtolower(char * s)
{
    char* strPtr = s;
    
    while (*strPtr != '\0')
    {
        *strPtr = tolower (*(strPtr ++));
    }
    return s;
}
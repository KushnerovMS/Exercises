#include <stdio.h>
#include <stdlib.h>

#define N 1000

int main()
{
    char s1[N + 1] = {};
    char s2[N + 1] = {};
    char maxLen = 0;
    
    while (scanf ("%1000s", s1) == 1)
    {
        int len = strlen (s1);
        if (len > maxLen)
        {
            maxLen = len;
            char* s = s1;
            s1 = s2;
            s2 = s;
        }
    }
    
    printf("Hello world!\n");
    return 0;
}
